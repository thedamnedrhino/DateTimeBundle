    "license": "BSD",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Mohamad Mohebifar",
            "homepage": "https://mohebifar.com"
        },
        {
            "name": "Masoud Zohrabi",
            "homepage": "http://mdzzohrabi.ir",
            "email": "mdzzohrabi@gmail.com"
        }
    ],
    "archive": {},
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.0.16,~2.0|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "symfony/form": "~2.3|~3.0"
    },

    "require-dev": {},

    "suggest": {},

    "autoload": {
        "psr-0": {"Mohebifar\\DateTimeBundle": ""}
    },

    "target-dir": "Mohebifar/DateTimeBundle"
}
